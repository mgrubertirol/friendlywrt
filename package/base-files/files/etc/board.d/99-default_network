. /lib/functions/uci-defaults.sh

board_config_update

json_is_a network object && exit 0

ucidef_set_interface_lan 'eth0'
ucidef_set_interface_wan 'wwan0'

board_config_flush

#switch sys led to green
echo 255 > /sys/class/leds/iduv2:green:sys/brightness

exit 0
